{
  "extends": ["plugin:prettier/recommended"],
  "plugins": ["prettier", "import"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "shared-node-browser": true,
    "browser": true
  },
  "rules": {
    "no-var": "error",
    "prefer-const": "error",
    "import/extensions": [
      "error",
      {
        "js": "always"
      }
    ]
  }
}

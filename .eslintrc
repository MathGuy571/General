{
  "extends": ["plugin:prettier/recommended"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "shared-node-browser": true,
    "browser": true
  },
  "rules": {
    "no-var": "error",
    "prefer-const": "error"
  }
}
